{
    "name": "drupal4gov/drupal4gov.us",
    "description": "Drupal4Gov Project.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "acquia/blt": "^11.0",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/lightning": "^4.0",
        "acquia/memcache-settings": "*",
        "davereid/drush-acquia-hook-invoke": "dev-master",
        "drupal-composer/drupal-scaffold": "^2.5.4",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/adminimal_admin_toolbar": "~1.0",
        "drupal/cog": "^1.0.0",
        "drupal/config_ignore": "2.x-dev",
        "drupal/config_split": "^1.0.0",
        "drupal/core": "^8.8.0",
        "drupal/devel": "^2.1",
        "drupal/easy_breadcrumb": "^1.6",
        "drupal/environment_indicator": "~3",
        "drupal/field_group": "^3.0",
        "drupal/google_analytics": "^2.2",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/paragraphs": "^1.2",
        "drupal/qa_accounts": "^1.0.0-alpha1",
        "drupal/recaptcha": "^2.2",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2",
        "hirak/prestissimo": "^0.3",
        "php": ">=7.3",
        "typhonius/acquia_cli": "^1",
        "mglaman/drupal-check": "1.0.14"
    },
    "require-dev": {
        "acquia/blt-require-dev": "11.x-dev"
    },
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patches": {
            "acquia/blt": {
                "fix config import errors": "https://github.com/acquia/blt/pull/4068.patch"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush",
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
